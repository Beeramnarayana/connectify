{
  "version": 2,
  "name": "social-media",
  "builds": [
    {
      "src": "BackEnd/Index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BackEnd/Index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/dist/$1"
    }
  ]
}
